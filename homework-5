            console.log(a.concat(b));
    }
    function function2() {
        var input = document.getElementById("out");
        var arr2 = input.value;
        var arr = [10,3,7,11,12,30];
            console.log(arr.concat(arr2));
    }
    function function3() {
        var arr = [10,3,7,11,12,30];
            arr = [10,3,7,11,12,30]
            arr.shift("10");
            console.log(arr)
    }
    function function4() {
        var arr = [10,3,7,11,12,30];
            arr = [10,3,7,11,12,30]
